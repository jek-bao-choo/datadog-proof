{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ],
      "env": {}
    },
    "playwright-mcp": {
      "command": "npx",
      "args": [
          "@playwright/mcp@latest",
          "--caps=vision",
          "--browser=chrome"
      ]
    }
  }
}